<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.prototipo2tt">
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Prototipo2tt">
        <activity android:name=".MainActivity" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".login" android:screenOrientation="portrait"/>
        <activity android:name=".registrar_alumno" android:screenOrientation="portrait"/>
        <activity android:name=".alumno_home" android:screenOrientation="portrait"/>
        <activity android:name=".login_profesor" android:screenOrientation="portrait"/>
        <activity android:name=".profesor_home" android:screenOrientation="portrait"/>
    </application>

</manifest>